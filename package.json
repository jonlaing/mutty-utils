{
  "name": "mutty-utils",
  "version": "1.0.2",
  "description": "type and firestore utils for mutty",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.17.12",
    "@babel/plugin-transform-modules-commonjs": "^7.17.12",
    "@microsoft/tsdoc": "^0.14.1",
    "@microsoft/tsdoc-config": "^0.16.1",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.34",
    "@types/ramda": "^0.28.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.7.0",
    "ts-toolbelt": "^9.6.0",
    "typedoc": "^0.22.15",
    "typedoc-plugin-markdown": "^3.12.1",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@firebase/app-compat": "^0.1.25",
    "@firebase/app-types": "^0.7.0",
    "firebase": "8.2.3",
    "firebase-admin": "^10.2.0",
    "moment": "^2.29.3",
    "ramda": "^0.28.0",
    "shonad": "https://github.com/jonlaing/shonad",
    "uuid": "^8.3.2"
  },
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint --ext .ts ./src",
    "format": "prettier --ignore-path .gitignore --write \"./src/**/*.+(js|ts|json)\"",
    "build": "tsc -p tsconfig.build.json",
    "build:docs": "typedoc --options typedoc.md.json",
    "uglify": "find ./dist/  -iname '*.js' -exec uglifyjs --compress --comments /sourceMappingURL/ -o {} -- {} \\;",
    "test": "jest"
  }
}
